program prog7;
uses crt;

var
//Используемые переменные
x1,x2: integer;
a,p:real;

begin
//Задаем начальные и конечные значения переменным,которые принимают участие
//в работе функции
x1:=5;
x2:=45;
a:=1.9;

while(x1<=x2)
do
	begin
	//Тут реализация функции. Сразу печатаем значение X аргумента и функции f(x)
	//при заданных значениях X
	//Под корнем каждый второй раз образуется отрицательный член - т.е. берем модуль abs
	     p:=  sin((a*sqr(x1))/(x1+1));//Этот член функции может принимать и отрицательные значение,
	     //но нам нужны только положительные:
	     if (p>=0) then
	        begin
               writeln('x=',x1,';   y(x)=',(x1-a)*(sqr(x1)+1)*sqrt(p),';');
          end
       else
          begin
            writeln('При x=',x1,' под корнем отрицательное значение');
          end;
       
       //Новое значение Х
       x1:=x1+5;
	end;
end.

