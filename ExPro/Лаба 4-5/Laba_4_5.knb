    <Args>0</Args>
    <Args>1</Args>
    <Args>2</Args>
    <Args>3</Args>
    <Act>ВЫВОД("Анализируем блок с координатами: ",х," ",у)</Act>
    <Act>тип=блок.Тип</Act>
    <Act>ВЫВОД("Тип: ",тип)</Act>
    <Act>текущий_блок=блок</Act>
    <Act>берем_х=х</Act>
    <Act>берем_у=у</Act>
    <Act>количество_блоков=КОЛЭЛЕМ(блок.Блоки)</Act>
    <Act>ВЫВОД("Количество подблоков: ",количество_блоков)</Act>
    <Act>ждать_посл=1</Act>
    <Act>ПОКА(И(тип=="последовательный",ждать_посл==1))</Act>
    <Act>длина_ветки=длина_ветки</Act>
    <Act>РисоватьПоследовательныйБлок(текущий_блок,берем_х,берем_у,длина_ветки)</Act>
    <Act>ждать_посл=0</Act>
    <Act>ВЫВОД("---2---")</Act>
    <Act>ВЫХОД</Act>
    <Act>ждать_парал=1</Act>
    <Act>ПОКА(И(тип=="параллельный",ждать_парал==1))</Act>
    <Act>берем_у=у/2</Act>
    <Act>длина_ветки=длина_ветки-100</Act>
    <Act>РисоватьПараллельныйБлок(текущий_блок,берем_х,берем_у,длина_ветки)</Act>
    <Act>ждать_парал=0</Act>
    <Act>ВЫВОД("---1---")</Act>
    <Act>ВЫХОД</Act>
    <Comment>Рисует переданный блок</Comment>
