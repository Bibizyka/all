      <StartValue />
      <Opredelena>false</Opredelena>
    </LocalVariables>
    <LocalVariables xsi:type="Number" Name="отсчет_высоты" Goal="false">
      <IsBackward>false</IsBackward>
      <Goal>False</Goal>
      <Comment />
      <StartValue />
      <Opredelena>false</Opredelena>
    </LocalVariables>
    <Args>0</Args>
    <Args>1</Args>
    <Args>2</Args>
    <Args>3</Args>
    <Act>ВЫВОД("=&gt;Рисуем параллельный блок: ",х,", ",у)</Act>
    <Act>количество_блоков=КОЛЭЛЕМ(блок.Блоки)</Act>
    <Act>ВЫВОД("-&gt;Количество блоков в параллельном блоке: ",количество_блоков)</Act>
    <Act>счетчик=0</Act>
    <Act>высота=у-50*(количество_блоков-1)/2</Act>
    <Act>отсчет_высоты=50*количество_блоков-50</Act>
    <Act>сдвиг_по_х=х+50</Act>
    <Act>ПОКА(И(количество_блоков==3,счетчик==0))</Act>
    <Act>счетчик=1</Act>
    <Act>РИСЛИНИЯ(х,высота+50,сдвиг_по_х,высота+50,"Red")</Act>
    <Act>РИСЛИНИЯ(х+длина_ветки+50,высота+50,сдвиг_по_х+длина_ветки+50,высота+50,"Red")</Act>
    <Act>отсчет_высоты=отсчет_высоты+50</Act>
    <Act>ВЫХОД</Act>
    <Act>счетчик=0</Act>
    <Act>ПОКА(И(количество_блоков==2,счетчик==0))</Act>
    <Act>счетчик=1</Act>
    <Act>РИСЛИНИЯ(х,высота+25,сдвиг_по_х,высота+25,"Red")</Act>
    <Act>РИСЛИНИЯ(х+длина_ветки+50,высота+25,сдвиг_по_х+длина_ветки+50,высота+25,"Red")</Act>
    <Act>ВЫХОД</Act>
    <Act>счетчик=1</Act>
    <Act>ВЫВОД("-&gt; Сдвиг по Х: ",сдвиг_по_х)</Act>
    <Act>длина_ветки=длина_ветки</Act>
    <Act>ПОКА(счетчик&lt;=количество_блоков)</Act>
    <Act>текущий_блок=ЭЛЕМСП(счетчик,блок.Блоки)</Act>
    <Act>АнализироватьСоединение(текущий_блок,сдвиг_по_х,высота,длина_ветки)</Act>
    <Act>высота=высота+50</Act>
    <Act>счетчик=счетчик+1</Act>
    <Act>ВЫХОД</Act>
    <Act>РИСЛИНИЯ(сдвиг_по_х,высота-50,сдвиг_по_х,высота-50-отсчет_высоты,"Red")</Act>
    <Act>РИСЛИНИЯ(сдвиг_по_х+длина_ветки,высота-50,сдвиг_по_х+длина_ветки,высота-50-отсчет_высоты,"Red")</Act>
    <Act>ВЫВОД("&lt;=Вышли из рисования параллельного блока")</Act>
    <Act>ВЫВОД(" ")</Act>
