      <Goal>False</Goal>
      <Comment />
      <StartValue />
      <Opredelena>false</Opredelena>
    </LocalVariables>
    <Args>0</Args>
    <Act>R=0</Act>
    <Act>количество_блоков=КОЛЭЛЕМ(блок.Блоки)</Act>
    <Act>счетчик=1</Act>
    <Act>ПОКА(счетчик&lt;=количество_блоков)</Act>
    <Act>текущий_блок=ЭЛЕМСП(счетчик,блок.Блоки)</Act>
    <Act>ВЫВОД(текущий_блок)</Act>
    <Act>ВЫВОД(текущий_блок.R)</Act>
    <Act>R=R+текущий_блок.R</Act>
    <Act>счетчик=счетчик+1</Act>
    <Act>ВЫХОД</Act>
    <Act>Возврат(R)</Act>
