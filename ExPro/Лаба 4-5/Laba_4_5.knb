  <Comment />
  <Rule Name="Создание цепи">
    <Act>цепь=Цепь</Act>
    <Act>блок_цепи=Блок</Act>
    <Act>блок1=Блок</Act>
    <Act>блок1_1=Блок</Act>
    <Act>резистор_1=Резистор</Act>
    <Act>резистор_2=Резистор</Act>
    <Act>блок1_1.Тип="последовательный"</Act>
    <Act>блок1_1.Блоки=[резистор_1,резистор_2]</Act>
    <Act>блок1_2=Блок</Act>
    <Act>резистор_3=Резистор</Act>
    <Act>блок1_2.Тип="последовательный"</Act>
    <Act>блок1_2.Блоки=[резистор_3]</Act>
    <Act>блок1.Блоки=[блок1_1,блок1_2]</Act>
    <Act>блок1.Тип="параллельный"</Act>
    <Act>блок2=Блок</Act>
    <Act>резистор_4=Резистор</Act>
    <Act>блок2.Тип="последовательный"</Act>
    <Act>блок2.Блоки=[резистор_4]</Act>
    <Act>блок3=Блок</Act>
    <Act>резистор_5=Резистор</Act>
    <Act>блок3.Тип="последовательный"</Act>
    <Act>блок3.Блоки=[резистор_5]</Act>
    <Act>блок_цепи.Блоки=[блок1,блок2,блок3]</Act>
    <Act>блок_цепи.Тип="параллельный"</Act>
    <Act>цепь.Блок=блок_цепи</Act>
    <Enable>true</Enable>
    <Priority>50</Priority>
  </Rule>
  <Rule Name="Рисование цепи">
    <Act>НарисоватьЭлектрическуюЦепь(цепь)</Act>
    <Enable>true</Enable>
    <Priority>50</Priority>
  </Rule>
  <Rule Name="Ввод значений цепи">
    <Act>R1=0</Act>
    <Act>R2=0</Act>
    <Act>R3=0</Act>
    <Act>R4=0</Act>
    <Act>R5=0</Act>
    <Act>U=0</Act>
    <Act>ВВОДБЛ("Введите указанные сопротивления и напряжение в цепи:",R1,R2,R3,R4,R5,U)</Act>
    <Act>резистор_1.R=R1</Act>
    <Act>резистор_2.R=R2</Act>
    <Act>резистор_3.R=R3</Act>
    <Act>резистор_4.R=R4</Act>
    <Act>резистор_5.R=R5</Act>
    <Enable>true</Enable>
    <Priority>50</Priority>
  </Rule>
  <Rule Name="Расчет общего сопротивления цепи">
    <Act>R=РассчитатьСопротивлениеЦепи(цепь)</Act>
    <Act>ВЫВОД("Общее сопротивление цепи R = ",R," Ом")</Act>
    <Enable>true</Enable>
    <Priority>50</Priority>
  </Rule>
  <Rule Name="Расчет силы тока цепи">
    <Act>I=U/R</Act>
    <Act>ВЫВОД("Сила тока в цепи 'I' = ",I," Ампер")</Act>
    <Enable>true</Enable>
    <Priority>50</Priority>
  </Rule>
  <Rule Name="Вывод данных">
    <Act>док=ДОКУМЕНТ(ШаблонДокумента)</Act>
    <Act>ЗАПИСЬ(док,"Results.htm")</Act>
    <Enable>true</Enable>
    <Priority>50</Priority>
  </Rule>
